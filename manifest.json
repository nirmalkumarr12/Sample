{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "Nirmal's first extension",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs", "http://www.google.com/*","https://www.quora.com/*"
     ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "css":["mystyle.css"],
      "js": ["myscript.js"]
      
    }
  ],
   "chrome_url_overrides": {
    "newtab": "popup.html"
      
    
  }
}
